{
  "name": "state-decorator",
  "version": "6.9.0",
  "description": "React state management library",
  "main": "index.js",
  "module": "es/index.js",
  "types": "index.d.ts",
  "private": true,
  "repository": "git@github.com:GlobalSport/state-decorator.git",
  "author": "Damien Garbarino <d.garbarino@mycoachsport.com>",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "test": "jest tests/*",
    "bump-beta": "yarn test && yarn clean && yarn rollup && bump --commit 'build: release v%s' && yarn copy",
    "bump-version": "yarn test && yarn clean && yarn rollup && bump --commit 'build: release v%s' --tag --push && yarn copy",
    "clean": "rimraf ./dist",
    "copy": "copyfiles -f package.json README.md LICENSE lib && json -I -f lib/package.json -e \"this.private=false; this.devDependencies=undefined; this.optionalDependencies=undefined; this.scripts=undefined; this.husky=undefined; this['lint-staged']=undefined; \"",
    "build": "yarn rollup && yarn copy",
    "analyze": "cross-env NODE_ENV=analyze rollup -c rollup.config.js",
    "rollup": "cross-env NODE_ENV=production rollup -c rollup.config.js",
    "tslint": "tslint -p ./tsconfig.json --fix"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@swc/cli": "^0.1.55",
    "@swc/core": "^1.2.121",
    "@swc/jest": "^0.2.15",
    "@testing-library/react-hooks": "^5.1.0",
    "@types/jest": "^26.0.0",
    "@types/react": "^16.9.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "copyfiles": "^2.2.0",
    "cross-env": "^5.2.0",
    "husky": "^1.0.0-rc.15",
    "jest": "^27.0.0",
    "json": "^9.0.6",
    "lint-staged": "^7.3.0",
    "prettier": "^2.1.5",
    "react": "^16.9.0",
    "react-dom": "16.9.0",
    "rollup": "^2.41.2",
    "rollup-plugin-strip-banner": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-visualizer": "^5.5.4",
    "tslib": "^2.3.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-react-hooks": "^2.0.0",
    "typescript": "^4.3.5",
    "version-bump-prompt": "^5.0.3"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "lint-staged": {
    "src/*.{ts,tsx}": [
      "tslint -p ./tsconfig.json -e 'tests/**' -e 'dist/**'",
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [
    "state",
    "typescript",
    "react",
    "javascript"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "fast-deep-equal": "^3.1.3"
  },
  "resolutions": {
    "minimist": "^1.2.3",
    "acorn": "^6.4.1",
    "kind-of": "^6.0.3",
    "lodash": "4.17.19",
    "path-parse": "1.0.7"
  }
}
