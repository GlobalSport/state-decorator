{
  "name": "state-decorator",
  "version": "5.0.0-beta.1",
  "description": "React state management component",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "repository": "git@github.com:GlobalSport/state-decorator.git",
  "author": "Damien Garbarino <d.garbarino@mycoachsport.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest tests",
    "bump-version": "yarn test && yarn clean && yarn build && yarn rimraf ./dist/useStateDecorator && bump --commit 'build: release v%s' --tag --push",
    "clean": "rimraf ./dist",
    "build": "yarn build:hooks && yarn build:render-props",
    "build:hooks": "cross-env NODE_ENV=production rollup -c rollup.config.js",
    "build:render-props": "cross-env NODE_ENV=production rollup -c rollup.config.renderprops.js",
    "tslint": "tslint -p ./tsconfig.json --fix"
  },
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.0",
    "@babel/node": "^7.7.0",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/plugin-transform-modules-commonjs": "^7.7.5",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/preset-typescript": "^7.7.2",
    "@types/jest": "^24.0.17",
    "@types/react": "^16.4.14",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^23.0.1",
    "cross-env": "^5.2.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "husky": "^1.0.0-rc.15",
    "immer": "^1.7.2",
    "jest": "^24.8.0",
    "lint-staged": "^7.3.0",
    "prettier": "^1.19.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "rollup": "^1.14.3",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.1",
    "rollup-plugin-terser": "^5.0.0",
    "rollup-plugin-typescript2": "^0.21.1",
    "rollup-plugin-visualizer": "^0.9.2",
    "ts-jest": "^24.0.0",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-react-hooks": "^2.0.0",
    "typescript": "^3.4.5",
    "version-bump-prompt": "^5.0.3"
  },
  "peerDependencies": {
    "react": "^16.4.0",
    "react-dom": "^16.4.0"
  },
  "lint-staged": {
    "src/*.{ts,tsx}": [
      "tslint -p ./tsconfig.json -e 'tests/**' -e 'dist/**'",
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [
    "state",
    "typescript",
    "react",
    "javascript"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "fast-deep-equal": "^2.0.1"
  },
  "resolutions": {
    "@babel/helpers": "^7.6.0"
  }
}